<%- include('partials/header.ejs'); -%>
<h1>Home</h1>
<p><%= homepageContent %></p>
<% posts.forEach(function(post){ %>
    <h1><%=post.title%></h1>
    <%var link = "/posts/"+post.title%>
    <p><%=post.content.substring(0,100) + "..."%><a href=<%=link%>>Read more</a> </p>
<% }); %>
<%- include('partials/footer.ejs'); -%>